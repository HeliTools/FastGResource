{
  "manifest_version": 2,
  "name": "FastGResources",
  "description": "This extension use google.cn and fonts.lug.ustc.edu.cn and fonts.useso.com to fast ur Climbing.",
  "version": "1.0",
  "permissions": [
  	"webRequest",
  	"webRequestBlocking",
    "*://inf.pub/*",
    "https://inf.pub/*",
    "*://fonts.googleapis.com/*",
    "*://ajax.googleapis.com/*",
    "https://fonts.googleapis.com/*",
    "*://*.googleusercontent.com/*"
  ],
  "background":{
  	"scripts":[
  		"background.js"
  	]
  }
}
